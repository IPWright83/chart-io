{
  "name": "@chart-it",
  "version": "1.0.0",
  "packageManager": "pnpm@7.12.1",
  "type": "module",
  "author": "\"Ian Wright\" <\"ipwright83+dev@gmail.com\">",
  "scripts": {
    "bump": "changeset version",
    "changeset": "changeset",
    "test": "turbo run test -- --watchAll=false",
    "test:watch": "node scripts/choice test --watchAll=true",
    "test:package": "node scripts/choice test --watchAll=false",
    "storybook:watch": "node scripts/choice storybook",
    "storybook:package": "node scripts/choice build-storybook",
    "storybook:deploy": "pnpm --filter @chart-it/react-d3 deploy-storybook",
    "clean": "rm -rf node_modules && pnpm -r clean",
    "refresh": "pnpm clean && pnpm install",
    "build": "turbo run build",
    "build:watch": "turbo run build --scope=@iw* --no-deps -- -- -w",
    "graph": "turbo run build --graph=graph.html",
    "buildp": "node scripts/choice build",
    "dev": "turbo run build --scope=@iw* --no-deps && turbo run dev --no-cache --parallel --continue",
    "lint": "turbo run lint",
    "lintp": "node scripts/choice lint"
  },
  "np": {
    "publish": false,
    "tests": false
  },
  "devDependencies": {
    "@changesets/cli": "^2.24.2",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/node-logger": "^6.5.9",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.9",
    "@storybook/storybook-deployer": "^2.8.12",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "babel-jest": "27.5.1",
    "babel-plugin-named-exports-order": "^0.0.2",
    "canvas": "^2.9.3",
    "chromatic": "^6.7.0",
    "crypto": "^1.0.1",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^9.0.2",
    "jest": "27.5.1",
    "jest-environment-jsdom": "27.5.1",
    "jest-image-snapshot": "^5.1.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^18.2.0",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.4.1",
    "rollup": "^2.77.2",
    "turbo": "^1.3.4",
    "web-vitals": "^2.1.4",
    "webpack": "5"
  },
  "pnpm": {
    "patchedDependencies": {
      "@changesets/git@1.5.0": "patches/@changesets__git@1.5.0.patch"
    }
  }
}
